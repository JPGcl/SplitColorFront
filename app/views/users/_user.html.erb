<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= user.phone %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= user.password %>
  </p>

  <p>
    <strong>User type:</strong>
    <%= user.user_type_id %>
  </p>

  <p>
    <strong>Login type:</strong>
    <%= user.login_type_id %>
  </p>

  <p>
    <strong>Plan:</strong>
    <%= user.plan_id %>
  </p>

  <p>
    <strong>Enabled:</strong>
    <%= user.enabled %>
  </p>

  <p>
    <strong>Confirm email:</strong>
    <%= user.confirm_email %>
  </p>

  <p>
    <strong>Registration date:</strong>
    <%= user.registration_date %>
  </p>

  <p>
    <strong>Discharge date:</strong>
    <%= user.discharge_date %>
  </p>

  <p>
    <strong>Confirmation email date:</strong>
    <%= user.confirmation_email_date %>
  </p>

</div>
